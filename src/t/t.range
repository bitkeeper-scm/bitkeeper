bk delta $Q -fy1.2.1.1 foo
bk edit $Q -M foo
bk delta $Q -fy1.4 foo
bk commit $Q -yfoo
# all versions plus the blank version
bk prs -r.. -hnd:I: foo > LIST
echo >> LIST
# matrix test
cat LIST | while read from; do
	cat LIST | while read to; do
		printf $from..$to
		bk prs -r$from..$to -hd' :I:' foo
